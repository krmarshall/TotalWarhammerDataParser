{
  "name": "totalwarhammerdataparser",
  "version": "0.1.0",
  "description": "Tool for extracting and parsing character skill trees into usable json.",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src",
    "lintFix": "eslint ./src --fix",
    "copySource": "cd ./bins && copySource.bat",
    "start": "node --max-old-space-size=8192 ./src/index.js",
    "startProd": "SET NODE_ENV=production&&node --max-old-space-size=8192 ./src/index.js",
    "profile": "node --prof --max-old-space-size=8192 ./src/index.js",
    "fullBuild": "run-s copySource start",
    "fullBuildProd": "run-s copySource startProd",
    "img": "SET IMG=true&&node --max-old-space-size=8192 ./src/index.js"
  },
  "author": "krmarshall",
  "license": "ISC",
  "dependencies": {
    "csv-parse": "^5.0.4",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  }
}
